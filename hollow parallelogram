  /*
    **********
   *        *
  *        *
 *        *
**********

*/





#include<stdio.h>
int main()
{
    int i,j,n,space,m;
    printf("enter m & n:");
    scanf("%d%d",&m,&n);
    for(i=1;i<=n;i++){
        for(space=1;space<=n-i;space++){
            printf(" ");
        }
        for(j=1;j<=m;j++){
                if(i==1||i==n||j==1||j==m)
            printf("*");
        else
            printf(" ");
        }
    printf("\n");
    }

    return 0;
}
