#include<stdio.h>
int main()
{
    int i,j,n;
    printf("enter n :");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            if(i==n||j==1)
                printf("*");
            else
                printf(" ");
        }
printf("\n");
    }
}

KKKKKKKKKKKKKK

#include<stdio.h>
int main()
{
    int i,j,n,x=1,m;
    printf("enter n and m:");
    scanf("%d%d",&n,&m);
    int y=m-1;

    for(i=1; i<=n; i++)
    {
        for(j=1; j<=m; j++)
        {
            if(j==1)
                printf("*");
            else if(i==j+1 && j>2)
                printf("*");
            else if((i==x && j==y) && j>1)
            {
                printf("*");
                x++;
                y--;
            }
            else
                printf(" ");
        }
      printf("\n");
    }

}

