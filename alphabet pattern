#include<stdio.h>
int main()
{
    int i,j,n;
    printf("enter n :");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            if(i==1)
                printf("*");
            else if(i==n&&j<n/2)
                printf("*");
            else if(j==n/2)
                printf("*");
            else
                printf(" ");
        }
printf("\n");
    }
}

