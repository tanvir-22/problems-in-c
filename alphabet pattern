#include<stdio.h>
int main()
{
    int i,j,n,m;
    printf("enter n and m:");
    scanf("%d%d",&n,&m);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=m;j++)
        {
            if(j==1 && i!=1 &&i!=n)
                printf("*");
            else if((i==1||i==n)&&(j>1))
                printf("*");

            else
                printf(" ");
        }
    printf("\n");

    }
return 0;
}
