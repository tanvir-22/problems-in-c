#include<stdio.h>
int main()
{
    int i,j,m,n;
    printf("enter n and m");
    scanf("%d%d",&n,&m);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=m;j++)
        {
            if(j==1||j==m)
                printf("*");
            else if(i==n/2+1&&(j>1&&j<m))
                printf("*");
            else
                printf(" ");
        }
printf("\n");
    }
}
