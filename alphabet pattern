#include<stdio.h>
int main()
{
    int i,j,n,m;
    printf("enter n : ");
    scanf("%d",&n);
    printf("enter m:");
    scanf("%d",&m);
    for(i=1; i<=n; i++)
    {
        for(j=1; j<=m; j++)
        {
            if(j==1&&i!=1)
                printf("*");
            else if(j==m&&i!=1)
                printf("*");
            else if((i==1||i==n/2)&&(j>1&&j<m))
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
    return 0;
}

