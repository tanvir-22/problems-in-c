#include<stdio.h>
int main()
{
    int i,j,n;
    printf("enter n :");
    scanf("%d%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            if(i==1||i==n)
                printf("*");
            else if(j==n/2+1)
                printf("*");
            else
                printf(" ");
        }
printf("\n");
    }
}


