//2.	Write a program that computes the following series using for loop: 10000+2000+400+â€¦+8

#include<stdio.h>
int main()

{

    int i=10000,sum=0;
    while(i>=8){
        sum=sum+i;
         i=i/5;
    }
    printf("%d",sum);
    return 0;
}
//using for loop
#include<stdio.h>
int main()
{
    int i,sum=0;
    for(i=10000;i>=8;i/=5)
    {
        sum=sum+i;
    }
    printf("%d",sum);
}
