7890
12345





*/
#include<stdio.h>
int main()
{
    int i,j,k=49,n;
    printf("enter N:");
    scanf("%d",&n);
    for(i=1;i<=n;i++){

    for(j=1;j<=i;j++,k++){
            printf("%c",k);
    if(k>=57)
        k=47;

    }
    printf("\n");
    }
    return 0;
}
/*
    1
   234
  56789
 0123456
789012345





*/
#include<stdio.h>
int main()
{
    int space,i,j,n,k=49;
    printf("enter any number:");
    scanf("%d",&n);
    for(i=1; i<=n; i++)
    {
        for(space=1; space<=n-i; space++)
        {
            printf(" ");
        }
        for(j=1; j<=2*i-1; j++,k++)
        {
            printf("%c",k);
            if(k>=57)
                k=47;

        }
        printf("\n");
    }
    return 0;
}
