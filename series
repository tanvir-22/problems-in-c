1/1+(1/1+1/2)+(1/1+1/2+1/3)+⋯+(1/1+1/2+⋯+1/n)


#include<stdio.h>
int main()
{
    int i,j,n;
    float sum=0.0,term;
    printf("enter N:");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        term=0;
        for(j=1;j<=i;j++)
            term=term+(1.0/j);
            sum+=term;
        
    }
    printf("%.3f",sum);
    return 0;
}
