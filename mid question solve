/*
B   D
C   E
D   F
E   G
 EFG
*/

#include<stdio.h>
int main()
{
    int i,j;
    for(i=1; i<=5; i++)
    {
        for(j=1; j<=5; j++)
        {
            if(j==1&&i!=5)
                printf("%c",65+i);
            else if(j==5&&i!=5)
                printf("%c",67+i);
            else if(i==5&&(j>1&&j<5))
                printf("%c",67+j);
            else
                printf(" ");

        }
        printf("\n");
    }
return 0;
}
